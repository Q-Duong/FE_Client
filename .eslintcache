      try {\n                \n                const res = await productAPI.getAll(`q=${searchTerm}`);\n                if(res.status === 200) {\n                    setSearchedProducts(res.data.data)\n                } else {\n                    console.log(res.data.message)\n                }\n            } catch (error) {\n                alert(error)\n            }\n        }\n        typingTimeoutRef.current = setTimeout(searching,200)\n    },[searchTerm])\n\n    return (\n        <div className={showSearchForm ? 'search-model search-display': 'search-model' } >\n            <div className=\"d-flex align-items-center justify-content-center\">\n                <div className=\"overlay\" onClick={handleSearchFormShow}></div>\n                <form onSubmit={(e) => handleSearch(e)} autocomplete=\"off\" className=\"search-model-form\">\n                    \n                    <div className=\"input_container\">\n                        <img src={search} className=\"input_img\"/>\n                        <input ref={inputRef} type=\"text\" id=\"keywords\"  name=\"keywords_submit\" placeholder=\"Tìm kiếm sản phẩm\" className=\"input_search\" value={searchTerm} onChange={(e) => setSearchTerm(e.target.value)} />\n                        <div className=\"search-close-switch\" onClick={handleSearchFormShow}>+</div>\n                    </div>\n                    <div className=\"search_seggest\">\n                        <p>GỢI Ý TÌM KIẾM</p>\n                        <ListGroup className=\"search_seggest\">\n                        {\n                            searchedProducts.map(product => (\n                                <ListGroup.Item onClick={() => handleClickSearchItem(product.id)}>\n                                   <FontAwesomeIcon className='search_icon' icon={faMagnifyingGlass} /> {product.name}\n                                </ListGroup.Item>\n                            ))\n                        }\n                        </ListGroup> \n                    </div>\n                    \n                    \n                </form>\n            </div>\n        </div>\n    );\n}\n\nexport default Search;","/Users/GEEKUp/Desktop/secret/FE_Client/src/components/Grid.jsx",[],"/Users/GEEKUp/Desktop/secret/FE_Client/src/components/Button.jsx",[],"/Users/GEEKUp/Desktop/secret/FE_Client/src/components/HeaderInfo.jsx",["250","251","252","253","254","255","256","257","258"],"import React from 'react';\nimport PropTypes from 'prop-types';\nimport { Dropdown } from 'react-bootstrap';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport { faCircleUser,faBox,faGear,faArrowRightFromBracket } from '@fortawesome/free-solid-svg-icons';\nimport cart from '../assets/images/icon/cart.png';\nimport heart from '../assets/images/icon/heart.png';\nimport CartPre from '../components/CartPre'\n\nHeaderUserInfo.propTypes = {\n    onLogout: PropTypes.func,\n};\n\nfunction HeaderUserInfo(props) {\n    const { onLogout, cartItems, token } = props\n\n    function handleLogout() {\n        if (onLogout) {\n            onLogout()\n        }\n    }\n\n    return (\n        <Dropdown.Menu>\n             <span className=\"caret\"></span>\n            {\n                    <>\n                        \n                        {\n                        cartItems.length!==0?\n                         <>\n                            {\n                                cartItems.map((item) => (\n                                    <Dropdown.Item className=\"cartDetails-top\" href=\"/\">\n                                        <CartPre item={item} key={item._id}/>\n                                    </Dropdown.Item>\n                                ))\n                            }\n                            <Dropdown.Item className=\"checkout\" href=\"/checkout\">\n                                <div className=\"checkout__button\">Thanh toán</div>  \n                            </Dropdown.Item>\n                        </>\n                        :\n                        <>\n                            <div className=\"cartDetails\">Giỏ hàng trống.</div>\n                        </>\n                        } \n                    </>\n                    \n            }\n        </Dropdown.Menu>\n\n    );\n}\n\nexport default HeaderUserInfo;","/Users/GEEKUp/Desktop/secret/FE_Client/src/pages/Home.jsx",["259","260"],"/Users/GEEKUp/Desktop/secret/FE_Client/src/pages/Catalog.jsx",[],"/Users/GEEKUp/Desktop/secret/FE_Client/src/pages/Register.jsx",[],"/Users/GEEKUp/Desktop/secret/FE_Client/src/pages/Category.jsx",["261","262"],"import React, { useState, useEffect, useRef } from 'react'\nimport { Button } from 'react-bootstrap';\nimport { useParams } from 'react-router-dom';\n\nimport { productAPI } from '../api/api';\nimport CheckBox from '../components/CheckBox';\nimport Helmet from '../components/Helmet'\nimport InfinityList from '../components/InfinityList'\n\n\n\nconst Catogory = () => {\n    const { id } = useParams();\n    const [products, setProducts] = useState([])\n\n    const initFilter = {\n        brands: []\n    }\n\n    const [productFilter, setProductFilter] = useState([])\n\n    const [filter, setFilter] = useState(initFilter)\n\n    const [brands, setBrands] = useState([])\n\n\n    const filterSelect = (checked, item) => {\n        if (checked) {\n            setFilter({ ...filter, brands: [...filter.brands, item._id] })\n        }\n        else {\n            const newBrand = filter.brands.filter(e => e !== item._id)\n            setFilter({ ...filter, brands: newBrand })\n        }\n    }\n\n\n    const clearFilter = () => setFilter(initFilter)\n    const filterRef = useRef(null)\n\n    const showHideFilter = () => filterRef.current.classList.toggle('active')\n\n    useEffect(() => {\n        async function getProducts() {\n            try {\n                const response = await productAPI.getByCategoryId(id);\n                const productDatas = response.data\n                const tempBrands = productDatas.reduce((arrBrands, product) => {\n                    if (!arrBrands.some(brand => brand._id == product.product.brand._id))\n                        arrBrands.push(product.product.brand)\n                    return arrBrands\n                }, [])\n                setProducts(productDatas)\n                setProductFilter(productDatas)\n                setBrands(tempBrands)\n            } catch (error) {\n                alert(error.response.data.message)\n            }\n        }\n        getProducts()\n    }, [id])\n\n    useEffect(\n        () => {\n            try {\n                let temp = [...products]\n                if (filter.brands.length > 0) {\n                    temp = temp.filter(e => filter.brands.includes(e.product.brand._id))\n                }\n                setProductFilter(temp)\n            } catch (error) { }\n        },\n        [filter]\n    )\n\n    return (\n        <Helmet title=\"Sản phẩm\">\n            <div className=\"catalog\">\n                <div className=\"catalog__filter\" ref={filterRef}>\n                    <div className=\"catalog__filter__close\" onClick={() => showHideFilter()}>\n                        <i className=\"bx bx-left-arrow-alt\"></i>\n                    </div>\n\n                    <div className=\"catalog__filter__widget\">\n                        <div className=\"catalog__filter__widget__title\">\n                            Hãng\n                        </div>\n                        <div className=\"catalog__filter__widget__content\">\n                            {\n                                brands.map((item) => (\n                                    <div key={item._id} className=\"catalog__filter__widget__content__item\">\n                                        <CheckBox\n                                            label={item.name}\n                                            onChange={(input) => filterSelect(input.checked, item)}\n     