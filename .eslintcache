oken,{exportOrder, purchaseProducts})\n            if(paymentMethod === 'INPERSON'){\n                alert('đặt hàng thành công')\n                dispatch(removeAll())\n                history.push('/order')\n            } else {\n                window.location.href= res.data.payUrl\n            }\n            \n        } catch (error) {\n            alert(error.response.data.message)\n        }\n    }\n\n    return (\n        \n\n        <Helmet title=\"Giỏ hàng\">\n            <section className=\"breadcrumb-option\">\n                <div className=\"container\">\n                    <div className=\"row\">\n                        <div className=\"col-lg-12\">\n                            <div className=\"breadcrumb__text\">\n                                <h3>Giỏ hàng của bạn</h3>\n                                <div className=\"breadcrumb__links\">\n                                    <Link to=\"/\">Trang chủ</Link>\n                                    <FontAwesomeIcon icon={faAngleRight} className=\"faAngleRight\" />\n                                    <span>Giỏ hàng của bạn</span>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </section>\n\n            {\n            cartItems.length > 0  ?\n                <>\n                    <div className=\"cart\">\n                        <div className=\"cart__list\">\n                            <div className=\"shopping__cart__table\">\n                                <table>\n                                    <thead>\n                                        <tr>\n                                            <th>Sản phẩm</th>\n                                            <th>Số lượng</th>\n                                            <th>Tạm tính</th>\n                                            <th></th>\n                                        </tr>\n                                    </thead>\n                                        {\n                                            cartItems.map((item) => (\n                                                <CartItem item={item} key={item._id}/>\n                                            ))\n                                        }\n                                </table>\n                            </div>\n                            \n                            <div className=\"continue__btn\">\n                                <Link to=\"/catalog\">\n                                    <Button className=\"continue__btn\">\n                                        Tiếp tục mua hàng\n                                    </Button>\n                                </Link>\n                            </div>\n                        </div>\n\n                        <div className=\"col-lg-4\">\n                            <div className=\"cart__discount\">\n                            </div>\n                            <div className=\"cart__total\">\n                                <h6>Tổng cộng</h6>\n                                <ul>\n                                    <li>Tạm tính: <span>{numberWithCommas(Number(totalBill))}</span></li>\n                                    <li>Giảm Giá :<span>0</span></li>\n                                    <li>Tổng tiền :<span>{numberWithCommas(Number(totalBill))}</span></li>\n                                    \n                                </ul>\n                                \n                                <Link to=\"/checkout\">\n                                    <Button className=\"check_out\">\n                                    Thanh toán\n                                    </Button>\n                                </Link>\n\n                            </div>\n                        </div>\n                    </div>\n                </>\n            :\n                <>\n                    <div class=\"container text-center\">