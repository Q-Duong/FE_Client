} />\n                                        {\n                                            cartItems.length !== 0 ?\n                                                <span className=\"header__menu__right__cart__btn__opiton__length\">{cartItems.length}</span>\n                                                : <></>\n                                        }\n\n                                    </Dropdown.Toggle>\n                                    <HeaderUserInfo cartItems={cartItems} token={token} onLogout={handleLogout} />\n                                </Dropdown>\n                            </div>\n                            <div className=\"header__menu__right__user__item\">\n                                    <img \n                                        src={user} \n                                        aria-controls={open ? 'basic-menu' : undefined}\n                                        aria-haspopup=\"true\"\n                                        aria-expanded={open ? 'true' : undefined}\n                                        onClick={handleClick}\n                                    />\n                                    <Menu\n                                        id=\"basic-menu\"\n                                        anchorEl={anchorEl}\n                                        open={open}\n                                        onClose={handleClose}\n                                        MenuListProps={{\n                                        'aria-labelledby': 'basic-button',\n                                        }}\n                                    >\n                                        {\n                                            token ?\n                                            <>\n                                                <MenuItem onClick={handleClose}>\n                                                    <Link to=\"/profile\">Xem thông tin</Link>\n                                                </MenuItem>\n                                                <MenuItem onClick={handleClose}>\n                                                    <Link to=\"/order\">Xem đơn hàng</Link>\n                                                </MenuItem>\n                                                <MenuItem onClick={() => {\n                                                    handleClose()\n                                                    dispatch(removeToken())\n                                                }}>\n                                                    Đăng xuất\n                                                </MenuItem>\n                                            </>:\n                                            <>\n                                                <MenuItem onClick={handleClose}>\n                                                    <Link to=\"/login\">Đăng nhập</Link>\n                                                </MenuItem>\n                                                <MenuItem onClick={handleClose}>\n                                                    <Link to=\"/register\">Đăng ký</Link>\n                                                </MenuItem>\n                                            </> \n                                        }\n                                        \n                                    </Menu>\n                            </div>\n                        </div>\n                    </div>\n                </Container>\n            </div>\n            <Search\n                showSearchForm={showSearchForm}\n                onSearchFormShow={handleSearchFormShow}\n                onSearch={handleSearch}\n            />\n        </>\n    )\n}\n\nexport default Header\n","/Users/quocduong/Desktop/FE_Client/src/routes/Routes.jsx",[],"/Users/quocduong/Desktop/FE_Client/src/api/api.js",[],"/Users/quocduong/Desktop/FE_Client/src/components/Grid.jsx",[],"/Users/quocduong/Desktop/FE_Client/src/components